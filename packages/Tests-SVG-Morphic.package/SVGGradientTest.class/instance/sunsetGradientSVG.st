^ '<svg viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
	<defs>
		<linearGradient id="myGradient">
			<stop offset="0%" stop-color="yellow" />
			<stop offset="95%" stop-color="red" />
		</linearGradient>
		<linearGradient id="sizedGradient" x1="0.5" x2="0.9" y1="0" y2="0.7">
			<stop offset="0%" stop-color="yellow" />
			<stop offset="95%" stop-color="red" />
		</linearGradient>
		<radialGradient id="radialGradient1">
			<stop offset="0%" stop-color="red" />
			<stop offset="100%" stop-color="blue" />
		</radialGradient>
		<radialGradient id="radialGradient2" cx="0.2" cy="0.3" r="0.5">
			<stop offset="0%" stop-color="red" />
			<stop offset="100%" stop-color="blue" />
		</radialGradient>
	</defs>

	<!-- using my linear gradient -->
	<circle cx="50" cy="50" r="40" fill="url(#myGradient)" transform="" />
	<rect x="100" y="0" width="100" height="100" fill="url(#radialGradient2)" />
</svg>'